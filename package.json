    "@react-three/cannon": "^4.6.1",
    "@react-three/drei": "^8.8.1",
    "@react-three/fiber": "^7.0.26",
    "antlr4": "^4.9.3",
    "mobx": "^6.3.13",
    "mobx-react": "^7.2.1",
    "react": "^18.0.0-rc.0-next-fe905f152-20220107",
    "react-contextmenu": "^2.14.0",
    "react-dom": "^18.0.0-rc.0-next-fe905f152-20220107",
    "three": "^0.137.5"
  },
  "devDependencies": {
    "@types/antlr4": "^4.7.2",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/three": "^0.137.0",
