{
  "name": "react-three-karol",
  "version": "1.0.0",
  "author": "Marco van Meegen",
  "homepage": "https://vanmeegen.github.io/react-three-karol/",
  "scripts": {
    "dev": "vite",
    "test": "vitest --run",
    "test:watch": "vitest",
    "coverage": "vitest --coverage",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prettier": "prettier -w src",
    "deploy": "yarn build && gh-pages -d dist",
    "generate:glb": "cd src/components/modelwrapper && npx gltfjsx --types ../../../public/<insertmyglbname>.glb"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/icons-material": "7.2.0",
    "@mui/material": "7.2.0",
    "@react-three/cannon": "^6.0.0",
    "@react-three/drei": "^10.5.1",
    "@react-three/fiber": "^9.2.0",
    "antlr4": "4.9.3",
    "browser-fs-access": "^0.38.0",
    "mobx": "6.13.7",
    "mobx-react": "7.6.0",
    "react": "^19.1.0",
    "react-blockly": "9.0.0",
    "react-contextmenu": "^2.14.0",
    "react-dom": "^19.1.0",
    "three": "^0.178.0",
    "use-sync-external-store": "1.2.0"
  },
  "devDependencies": {
    "@types/antlr4": "^4.7.2",
    "@types/node": "^17.0.23",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/three": "^0.178.1",
    "@types/use-sync-external-store": "^1",
    "@vitejs/plugin-react": "4.7.0",
    "gh-pages": "^6.3.0",
    "prettier": "^3.6.2",
    "typescript": "5.8.3",
    "vite": "7.0.5",
    "vitest": "3.2.4"
  },
  "packageManager": "yarn@4.9.2"
}
