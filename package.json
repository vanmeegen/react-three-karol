{
  "name": "react-three-karol",
  "version": "0.9.0",
  "author": "Marco van Meegen",
  "homepage": "https://vanmeegen.github.io/react-three-karol/",
  "scripts": {
    "dev": "vite",
    "test": "vitest --run",
    "test:watch": "vitest",
    "coverage": "vitest --coverage",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prettier": "prettier -w src",
    "deploy": "yarn build && gh-pages -d dist",
    "generate:glb": "cd src/components/modelwrapper && npx gltfjsx --types ../../../public/<insertmyglbname>.glb"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.3",
    "@react-three/cannon": "^6.0.0",
    "@react-three/drei": "^8.20.2",
    "@react-three/fiber": "^7.0.27",
    "antlr4": "^4.9.3",
    "browser-fs-access": "^0.25.3",
    "mobx": "^6.5.0",
    "mobx-react": "^7.3.0",
    "react": "^18.0.0",
    "react-blockly": "^7.0.0-alpha.2",
    "react-contextmenu": "^2.14.0",
    "react-dom": "^18.0.0",
    "three": "^0.139.2"
  },
  "devDependencies": {
    "@types/antlr4": "^4.7.2",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/three": "^0.139.0",
    "@vitejs/plugin-react": "^1.3.0",
    "gh-pages": "^3.2.3",
    "prettier": "^2.6.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vitest": "^0.8.1"
  },
  "packageManager": "yarn@4.9.2"
}
